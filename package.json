{
  "name": "@protocolnebula/ts-openapi-generator",
  "version": "2.0.0",
  "description": "Build API and models from Swagger/OpenAPI to use in any project type",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "start": "ts-node src/main.ts",
    "start:dev": "npx nodemon",
    "start:build": "node build/index.js",
    "start:webpack": "npx webpack --watch",
    "build": "tsc",
    "postbuild": "cp -r src/templates build",
    "build:webpack": "npx webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Ruben Arroyo Ceruelo",
    "email": "ruben@racs.es",
    "url": "https://racs.es"
  },
  "bin": {
    "transform-swagger": "./cli.js"
  },
  "homepage": "https://github.com/ProtocolNebula/ts-openapi-generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProtocolNebula/ts-openapi-generator"
  },
  "bugs": {
    "url": "https://github.com/ProtocolNebula/ts-openapi-generator/issues",
    "email": "project@hostname.com"
  },
  "dependencies": {
    "husky": "^4.3.6",
    "nodemon": "^2.0.6",
    "openapi-types": "^7.0.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.26",
    "case": "^1.6.1",
    "fs-extra": "^7.0.1",
    "fs": "0.0.1-security",
    "js-yaml": "^3.14.1",
    "mkdir-recursive": "^0.4.0",
    "mkdirp": "^0.5.1",
    "mustache": "^3.0.1",
    "rxjs": "^6.6.0",
    "ts-loader": "^8.0.1",
    "typescript": "^4.0.0-dev.20200724",
    "webpack-cli": "^3.3.12",
    "webpack": "^4.44.0",
    "yargs": "^12.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
